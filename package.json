{
  "name": "masterserver",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "client:build": "rollup -c apps/client/rollup.config.js",
    "client:watch": "rollup -c apps/client/rollup.config.js -w",
    "client:serve": "sirv dist/apps/client",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/masterserver/test/jest-e2e.json"
  },
  "dependencies": {
    "@flyover/box2d": "github:flyover/box2d.ts",
    "@nestjs/common": "^8.0.5",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.5",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/platform-express": "^8.0.5",
    "@nestjs/platform-ws": "^8.0.5",
    "@nestjs/websockets": "^8.0.5",
    "@pixi/tilemap": "^3.0.0-rc.1",
    "canvas": "^2.7.0",
    "jsdom": "^16.5.0",
    "on-property-change": "^2.1.3",
    "phaser": "^3.53.1",
    "pixi.js": "^6.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "simple-peer": "^9.10.0",
    "unique-names-generator": "^4.4.0",
    "uuid": "^8.3.2",
    "wrtc": "^0.4.7",
    "ws": "^8.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.0",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "@tsconfig/svelte": "^2.0.1",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.20",
    "@types/jsdom": "^16.2.7",
    "@types/node": "^16.0.0",
    "@types/simple-peer": "^9.6.2",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.6",
    "prettier": "^2.2.1",
    "rollup": "^2.40.0",
    "rollup-plugin-copy-watch": "^0.0.1",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "sirv-cli": "^1.0.11",
    "supertest": "^6.1.3",
    "svelte": "^3.35.0",
    "svelte-check": "^2.2.0",
    "svelte-preprocess": "^4.6.9",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.1.1",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslib": "^2.1.0",
    "typescript": "^4.1.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@shooter2D/shared/(.*)": "<rootDir>/libs/shared/src/$1",
      "@shooter2D/shared": "<rootDir>/libs/shared/src"
    }
  }
}
